#include <iostream>

using namespace std;

int main()
{
    double array[5],sum,mean;
    int    i=0;

    for(i=0;i<5;i++)
    {
     cout<<"\nEnter any value"<<endl;
     cin>>array[i];
    }

    for(i=0;i<5;i++)
    {
     sum=array[i]+sum;
    }

    mean=sum/5;

    cout<<"\nAverage value is"<<mean<< endl;
    return 0;
}
